view.data-v-0f7520f0, swiper.data-v-0f7520f0, swiper-item.data-v-0f7520f0 {
  box-sizing: border-box;
}
.commonPageBg.data-v-0f7520f0 {
  background: linear-gradient(to bottom, rgba(0, 0, 0, 0), white 550rpx), linear-gradient(to right, #beecd8, #f4e2d8);
}
.safe-area-inset-bottom.data-v-0f7520f0 {
  height: env(safe-area-inset-bottom);
}

